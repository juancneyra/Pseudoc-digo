Proceso MayorArreglo
	Definir array, i, mayor como entero
	Dimension array[10]
	mayor = 0
	Para i <- 0 hasta 9 Con Paso 1 Hacer
		Escribir "Ingrese un valor para su arreglo:"
		Leer array[i]
	FinPara
	
	Para i <- 0 hasta 9 Con Paso 1 Hacer
		Si array[i]> mayor Entonces
			mayor = array[i]
		FinSi
	FinPara
	Escribir  "El numero mayor es: " mayor
FinAlgoritmo
